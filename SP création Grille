//-------------Sous-programme création de la grille--------------//
int *InittableauGrille()
{
    int *tabGrille;
    tabGrille=(int*)calloc(1575,sizeof(int));           ///Le tableau est pour l'instant vide et on le remplira au fur et à mesure.
    for(int i=0;i<1575;i++)
    {
        tabGrille[i]=0;
    }
    return (tabGrille);
}
